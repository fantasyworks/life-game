{
  "name": "life-game",
  "version": "1.0.1",
  "description": "angular2で作られたcomponentと例",
  "author": "Mebro",
  "license": "ISC",
  "scripts": {
    "start": "concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "watch": "concurrently \"npm run tsc:w\" ",
    "less-compile-mac": "for file in $(find ./ ! -path './/node_modules/**' -name '*.less'); do lessc $file `dirname $file`'/'`basename $file | sed \"s|\\.less|\\.css|\"` ; done",
    "first": "npm run first:typings && npm run first:typedoc && npm run first:es6 && npm run first:typees6 && npm run first:typenumeral && npm run first:npmi",
    "first:typings": "npm install -g typings",
    "first:typedoc": "npm install -g typedoc",
    "first:es6": "npm install es6-promise es6-collections -g",
    "first:typees6": "typings install es6-promise es6-collections --save --global --source dt --global",
    "first:typenumeral": "typings install dt~numeraljs --global --save",
    "first:npmi": "npm i",
    "lite": "lite-server",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typedoc": "typedoc --out ./docs/ ./component/"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "@angular/upgrade": "~2.3.1",
    "@types/node": "^6.0.45",
    "angular-in-memory-web-api": "~0.1.13",
    "angular2-template-loader": "^0.5.0",
    "awesome-typescript-loader": "^2.2.4",
    "concurrently": "^3.0.0",
    "core-js": "^2.4.1",
    "es6-promise": "^4.0.5",
    "gulp": "^3.9.1",
    "gulp-less-to-scss": "0.0.9",
    "gulp-sass": "^2.3.2",
    "gulp.sass": "^1.3.9",
    "html-loader": "^0.4.4",
    "lite-server": "^2.2.2",
    "node-sass": "^2.1.1",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "^5.0.1",
    "script-loader": "^0.7.0",
    "systemjs": "^0.19.42",
    "typescript": "^2.0.10",
    "zone.js": "0.7.2"
  },
  "repository": [],
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.ts",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ]
}
